{
	"compilerOptions": {
		"target": "ES2021",
		"jsx": "react",
		"jsxFactory": "React.createElement",
		"jsxFragmentFactory": "React.Fragment",
		"module": "ES2022",
		"moduleResolution": "bundler",
		"types": ["bun-types"],
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	}
}
